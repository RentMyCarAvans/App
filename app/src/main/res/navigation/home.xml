<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home"
    app:startDestination="@id/homeFragment">

    <fragment
    android:id="@+id/homeFragment"
        android:name="com.avans.rentmycar.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_homeDetailFragment"
            app:destination="@id/homeDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeDetailFragment"
        android:name="com.avans.rentmycar.ui.home.HomeDetailFragment"
        android:label="HomeDetailFragment" >
        <argument
            android:name="id"
            app:argType="long" />
        <argument
            android:name="carmodel"
            app:argType="string" />
        <argument
            android:name="pickuplocation"
            app:argType="string" />
        <argument
            android:name="startDateTime"
            app:argType="string" />
        <argument
            android:name="endDateTime"
            app:argType="string" />
        <argument
            android:name="carImageUrl"
            app:argType="string" />
    </fragment>
</navigation>